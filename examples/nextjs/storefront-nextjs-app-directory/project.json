{
  "name": "storefront-nextjs-app-directory",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/nextjs/storefront-nextjs-app-directory",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/examples/nextjs/storefront-nextjs-app-directory"
      },
      "configurations": {
        "development": {
          "outputPath": "examples/nextjs/storefront-nextjs-app-directory"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "storefront-nextjs-app-directory:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "storefront-nextjs-app-directory:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "storefront-nextjs-app-directory:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "storefront-nextjs-app-directory:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "examples/nextjs/storefront-nextjs-app-directory/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "examples/nextjs/storefront-nextjs-app-directory/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}

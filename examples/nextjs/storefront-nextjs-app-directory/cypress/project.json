{
  "name": "storefront-nextjs-app-directory-e2e",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/nextjs/storefront-nextjs-app-directory/cypress/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "examples/nextjs/storefront-nextjs-app-directory/cypress/cypress.config.ts",
        "devServerTarget": "storefront-nextjs-app-directory:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "storefront-nextjs-app-directory:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "examples/nextjs/storefront-nextjs-app-directory/cypress/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["storefront-nextjs-app-directory"]
}
